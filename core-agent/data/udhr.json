  "articles": [
    {
      "id": "un-udhr-hr-cl-1",
      "number": "1",
      "heading": "Sva ljudska bića rađaju se slobodna i jednaka u dostojanstvu i pravima.",
      "text": "",
      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-2",
      "number": "2",
      "heading": "Svatko ima sva prava i slobode iz ove Deklaracije bez ikakve razlike.",
      "text": "",
      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-3",
      "number": "3",
      "heading": "Svatko ima pravo na život, slobodu i osobnu sigurnost.",
      "text": "",
      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-4",
      "number": "4",
      "heading": "Nitko ne smije biti držan u ropstvu ili podložan ropstvu ili trgovini",
      "text": "robljem.",
      "sentences": [
        "robljem."
      ],
      "hash": "e54600dafbd7d1427fcd77aad49fbf5ce47209612af0c013f74df7a17a85441c",
      "tokens_approx": 1,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-5",
      "number": "5",
      "heading": "Nitko ne smije biti podvrgnut mučenju, okrutnom, neljudskom ili",
      "text": "ponižavajućem postupanju ili kazni.",
      "sentences": [
        "ponižavajućem postupanju ili kazni."
      ],
      "hash": "c6f8a4a6e7ba9a4bbc1e2e80dfbe57ed999b66b3b7cc06e7040dce139cb99f61",
      "tokens_approx": 4,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-6",
      "number": "6",
      "heading": "Svatko ima pravo na priznavanje njegove pravne osobnosti.",
      "text": "",
      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-7",
      "number": "7",
      "heading": "Svi su jednaki pred zakonom i imaju pravo na jednaku zaštitu zakona.",
      "text": "",
      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-8",
      "number": "8",
      "heading": "Svatko ima pravo na djelotvoran pravni lijek pred nadležnim nacionalnim",
      "text": "sudovima.",
