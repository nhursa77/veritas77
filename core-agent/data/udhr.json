      "sentences": [],
      "hash": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855",
      "tokens_approx": 0,
      "refs": {},
      "notes": null
    },
    {
      "id": "un-udhr-hr-cl-11",
      "number": "11",
      "heading": "Svatko tko je optužen za kazneno djelo ima pravo da se smatra nevinim",
      "text": "dok mu se krivnja ne dokaže.",
      "sentences": [
